{
  "name": "combo",
  "version": "1.0.0",
  "type": "module",
  "description": "A parser combinator library designed to be simple to use",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "dev": "rslib build --watch",
    "types:check": "tsc --noEmit",
    "test:changed": "git diff --name-only main... | grep -E 'src/.*\\.ts$|tests/.*\\.test\\.ts$' | sed -e 's|src/\\(.*\\)\\.ts$|tests/\\1.test.ts|' -e 't' -e 'd' | xargs bun test"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rslib/core": "^0.4.0",
    "@types/bun": "^1.2.2",
    "@types/node": "^22.8.1",
    "typescript": "^5.7.3"
  },
  "private": true,
  "trustedDependencies": [
    "@biomejs/biome",
    "core-js"
  ]
}
